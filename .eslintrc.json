{
  "env": {
    "browser": true,
    "es2022": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": [
    "airbnb-base"
  ],
  "plugins": [
    "unicorn"
  ],
  "root": true,
  "rules": {
    "unicorn/prefer-node-protocol": "error",
    "no-console": "off",
    "require-await": "off",
    "no-restricted-syntax": "off",
    "no-await-in-loop": "off"
  }
}